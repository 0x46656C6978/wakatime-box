{
  "name": "wakatime-box",
  "version": "1.3.0",
  "description": "Show your WakaTime statistics in a pinned gist for your GitHub profile",
  "scripts": {
    "build": "rimraf dist/ && tsc src/index.ts --outDir dist && ncc build dist/index.js --minify",
    "lint": "eslint src/ --ext .ts --fix"
  },
  "keywords": [
    "wakatime",
    "wakatime-box",
    "pinned gist"
  ],
  "author": "Pavel Bychko <abordage.dev@gmail.com> (https://abordage.dev)",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "@actions/http-client": "^2.0.1",
    "@octokit/rest": "^19.0.7",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/node": "^18.14.1",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.35.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "ts-node": "^10.8.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  }
}
